mixin layout(title)
  doctype html
  meta(charset="utf-8")
  html(lang="en")
    head
      title= title
      link(rel="stylesheet", href="../assets/css/main.css")
      link(rel="stylesheet", href="../assets/css/prism.css")
  body
    .container
      block
    script(src="../assets/js/prism.js")



mixin handin(n, title)
  h3 Exercise 
    =n
    |  &mdash;  
    a(href="#exercise-#{n}")= title
  block
